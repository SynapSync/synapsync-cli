{
  "name": "@synapsync/cli",
  "version": "0.1.2",
  "description": "Neural AI Orchestration Platform - CLI for managing AI skills across multiple providers",
  "author": "SynapSync",
  "license": "MIT",
  "type": "module",
  "bin": {
    "synapse": "dist/index.js",
    "synapsync": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run build && npm test",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "cli",
    "ai",
    "llm",
    "claude",
    "openai",
    "gemini",
    "skills",
    "agents",
    "orchestration"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synapsync/cli.git"
  },
  "bugs": {
    "url": "https://github.com/synapsync/cli/issues"
  },
  "homepage": "https://synapsync.github.io/synapse-docs/",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "commander": "^12.1.0",
    "ora": "^8.1.0",
    "picocolors": "^1.1.1",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
